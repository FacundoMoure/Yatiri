[sub_resource type="GDScript" id="GDScript_ya4ey"]
script/source = "extends Node2D

@export var archer_scene: PackedScene   
@export var tribe_size: int = 3      
@export var spacing: float = 60.0     
@export var start_position: Vector2 = Vector2(100, 300)  
@export var attack_interval: float = 5.0  

var tribe: Array = []

func _ready() -> void:
	_spawn_tribe()
	_start_attack_timer()

func _spawn_tribe() -> void:
	for i in range(tribe_size):
		var archer = archer_scene.instantiate()
		add_child(archer)

		var random_offset = randf_range(-40.0, 40.0)
		archer.position = start_position + Vector2(i * spacing + random_offset, 0)
		tribe.append(archer)

		Global.add_tribe_member()
		
		if archer.has_signal(\"died\"):
			archer.died.connect(func():
				Global.remove_tribe_member())

func _start_attack_timer() -> void:
	var timer = Timer.new()
	timer.wait_time = attack_interval
	timer.autostart = true
	timer.one_shot = false
	add_child(timer)
	timer.timeout.connect(_on_attack_timeout)

func _on_attack_timeout() -> void:
	if get_tree().get_nodes_in_group(\"Enemy\").is_empty():
		return

	for archer in tribe:
		if archer and archer.is_inside_tree():
			var delay = randf_range(0.0, 6.5)  
			get_tree().create_timer(delay).timeout.connect(func():
				if archer and archer.is_inside_tree():
					archer.do_attack())  
"

